<Window x:Class="EM.IOC.Luncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EM.IOC.Luncher"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Grid x:Name="header" DockPanel.Dock="Top" Height="24" Background="Red"/>

        <Grid x:Name="bottom" DockPanel.Dock="Bottom" Height="24" Background="Yellow">
            <WrapPanel VerticalAlignment="Center">
                <Button Content="注册" Margin="10,0,0,0" Click="Button_Click"/>
                <Button Content="移除注册" Margin="10,0" Click="Button_Click_1"/>
            </WrapPanel>
        </Grid>
        <Grid x:Name="content" Background="Blue"/>
    </DockPanel>
</Window>
